stock SaveTruckerPoints()
{
 	new
	szFileStr[1024],
 	File: fHandle = fopen("TruckerPoints.cfg", io_write);
	for(new iIndex; iIndex < MAX_TPS; iIndex++)
	{
  		format(szFileStr, sizeof(szFileStr), "%f|%f|%f|%d|%d\r\n",
  		TruckerPointInfo[iIndex][tpPosX],
  		TruckerPointInfo[iIndex][tpPosY],
  		TruckerPointInfo[iIndex][tpPosZ],
  		TruckerPointInfo[iIndex][tpName],
  		//SafeZoneInfo[iIndex][szSize],
  		TruckerPointInfo[iIndex][tpPickupID]);
  		fwrite(fHandle, szFileStr);
	}
	return fclose(fHandle);
}

stock LoadTruckerPoints()
{
	if(!fexist("TruckerPoints.cfg")) return 1;

	new string[128],
	szFileStr[128],
	File: iFileHandle = fopen("TruckerPoints.cfg", io_read),
	iIndex;

	while(iIndex < sizeof(TruckerPointInfo) && fread(iFileHandle, szFileStr))
	{
		sscanf(szFileStr, "p<|>fffii",
		TruckerPointInfo[iIndex][tpPosX],
  		TruckerPointInfo[iIndex][tpPosY],
  		TruckerPointInfo[iIndex][tpPosZ],
  		TruckerPointInfo[iIndex][tpName],
  		//SafeZoneInfo[iIndex][szSize],
  		TruckerPointInfo[iIndex][tpPickupID]
	);
		format(string, sizeof(string), "%s\n\nAddress: %d %s", GetGasolineTypePoint(iIndex), iIndex, GetZoneName(TruckerPointInfo[iIndex][tpPosX],TruckerPointInfo[iIndex][tpPosY],TruckerPointInfo[iIndex][tpPosZ]));
		if(TruckerPointInfo[iIndex][tpPosX] != 0.0)
		{
			TruckerPointInfo[iIndex][tpPickupID] = CreateDynamicPickup(1239, 23, TruckerPointInfo[iIndex][tpPosX], TruckerPointInfo[iIndex][tpPosY], TruckerPointInfo[iIndex][tpPosZ]);
			TruckerPointInfo[iIndex][tpTextID] = CreateDynamic3DTextLabel(string, -1, TruckerPointInfo[iIndex][tpPosX], TruckerPointInfo[iIndex][tpPosY], TruckerPointInfo[iIndex][tpPosZ]+0.5,30.0);
		}
		++iIndex;
	}
	return fclose(iFileHandle);
}



