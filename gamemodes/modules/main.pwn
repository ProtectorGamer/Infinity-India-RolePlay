			SendClientMessage(playerid, COLOR_YELLOW, string);
		}
		else if(type == '3')
		{
			TruckerPointInfo[tpid][tpName] = 3;
			format(wynn, sizeof(wynn), "You have set TruckerPoint ID %d's Name type to 3.", tpid);
			SendClientMessage(playerid, COLOR_YELLOW, string);
