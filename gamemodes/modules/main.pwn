CMD:givevoucher(playerid, params[])
{
	new string[500];
	if(pData[playerid][pAdmin] < 6) return SCM(playerid, COLOR_SYNTAX, "You are not authorized to use this command.");
	if(!pData[playerid][pAdminDuty] && pData[playerid][pAdmin] < 7)
	{
	    return SCM(playerid, COLOR_SYNTAX, "This command requires you to be on admin duty. /aduty to go on duty.");
	}
	new targetid, choice[16], amount;
	if(sscanf(params, "us[16]d", targetid, choice, amount))
	{
		SendClientMessageEx(playerid, COLOR_WHITE, "USAGE: /givesprize [playerid/PlayerName] [Choice] [Amount]");
		SendClientMessageEx(playerid, COLOR_GREY, "Choice: 1 = Car Voucher, 2 = VIP Voucher");
		return 1;
	}
	if(amount < 1) return SendClientMessage(playerid, COLOR_GREY, "Amount cannot go below 1");
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified is disconnected.");
	}

	if(!strcmp(choice, "1", true))
	{
		pData[targetid][pVehVoucher] += amount;
		mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET Vehvoucher = %i", pData[playerid][pVehVoucher]);
		mysql_tquery(connectionID, queryBuffer);
		format(string, sizeof(string), "You have been successfully given %s an amount of %i Car voucher(s). That player has %i Car Voucher(s) in total.", GetRPName(targetid), amount, pData[targetid][pVehVoucher]);
		SCM(playerid, COLOR_AQUA, string);
		format(string, sizeof(string), "%s has given you %i Car voucher(s). You may check your total count of vouchers at '/myvouchers'.", GetRPName(playerid), amount);
		SCM(playerid, COLOR_AQUA, string);
	}
	if(!strcmp(choice, "2", true))
	{
	    pData[targetid][pVIPVoucher] += amount;
		mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET VIPvoucher = %i", pData[playerid][pVIPVoucher]);
		mysql_tquery(connectionID, queryBuffer);
		format(string, sizeof(string), "You have been successfully given %s an amount of %i VIP voucher(s). That player has %i VIP Voucher(s) in total.", GetRPName(targetid), amount, pData[targetid][pVIPVoucher]);
		SCM(playerid, COLOR_AQUA, string);
		format(string, sizeof(string), "%s has given you %i VIP voucher(s). You may check your total count of vouchers at '/myvouchers'.", GetRPName(playerid), amount);
		SCM(playerid, COLOR_AQUA, string);
	}
	return 1;
}

