GetClosestTruckerPoint(playerid)
{
	new
	    Float:distance[2] = {99999.0, 0.0},
	    index = -1;

	for(new i = 0; i < MAX_TPS; i ++)
	{
		if(IsValidDynamicPickup(TruckerPointInfo[i][tpPickupID]))
		{
			distance[1] = GetPlayerDistanceFromPoint(playerid, TruckerPointInfo[i][tpPosX], TruckerPointInfo[i][tpPosY], TruckerPointInfo[i][tpPosZ]);

			if(distance[0] > distance[1])
			{
