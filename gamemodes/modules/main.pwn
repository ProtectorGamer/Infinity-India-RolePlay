				ApplyAnimation(playerid, "ped", "FALL_collapse", 4.1, 0, 1, 1, 0, 0);

				//SetPlayerSpecialTag(playerid, TAG_BROKEN_LEG);

				pData[playerid][pBrokenLeg] = 1;

