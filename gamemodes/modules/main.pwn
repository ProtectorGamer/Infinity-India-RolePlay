CMD:joinbet(playerid, params[])
{
	new option[5], param[128], amount;
	if(!gSabongEnable)
	{
		return SCM(playerid, COLOR_SYNTAX, "Sabong is currently disabled.");
	}
	if(SabongVoted[playerid] == 1)
	{
		return SCM(playerid, COLOR_SYNTAX, "You already voted.");
	}
	if(sscanf(params, "s[5]S()[128]", option, param))
	{
		return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [Red/Blue]");
	}

	if(!strcmp(option, "red", true))
	{
		if(sscanf(param, "i", amount))
		{
			return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [red] [amount]");
		}
		if(pData[playerid][pCash] < amount)
		{
			return SCM(playerid, COLOR_SYNTAX, "You don't have that kind of amount for this option.");
		}
		
		if(SabongVoted[playerid] == 0)
		{
			SabongRed++;
			pData[playerid][pBet] = SabongRed;
			SabongRedValue += amount;
			SabongVoted[playerid] = 1;
			GivePlayerCash(playerid, -amount);
			SCM(playerid, COLOR_YELLOW, "You have placed your bet to "RED"Red.");
			SMA(COLOR_NEWBIE, "1 person has place their bet on "RED"Red"WHITE".");
			SMA(COLOR_NEWBIE, "------------[Red]----------");
			SMA(COLOR_NEWBIE, "Total Bet: %d 	Total Person Bet: %d.", SabongRedValue, SabongRed);
		}
	}
	if(!strcmp(option, "blue", true))
	{
		if(sscanf(param, "i", amount))
		{
			return SCM(playerid, COLOR_SYNTAX, "Usage: /bet [blue] [amount]");
		}
		if(pData[playerid][pCash] < amount)
		{
			return SCM(playerid, COLOR_SYNTAX, "You don't have that kind of amount for this option.");
		}
		
		if(SabongVoted[playerid] == 0)
		{
			SabongBlue++;
			pData[playerid][pBet] = SabongBlue;
			SabongBlueValue += amount;
			SabongVoted[playerid] = 1;
			GivePlayerCash(playerid, -amount);
			SCM(playerid, COLOR_YELLOW, "You have placed your bet to "BLUE"blue.");
			SMA(COLOR_NEWBIE, "1 person has place their bet on "BLUE"blue"WHITE".");
			SMA(COLOR_NEWBIE, "------------[Blue]----------");
			SMA(COLOR_NEWBIE, "Total Bet: %d 	Total Person Bet: %d.", SabongBlueValue, SabongBlue);
		}
	}
	return 1;
}

