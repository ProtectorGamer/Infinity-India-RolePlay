    if(!IsPlayerInRangeOfPoint(playerid, 5.0, GangInfo[pData[playerid][pGang]][gStashX], GangInfo[pData[playerid][pGang]][gStashY], GangInfo[pData[playerid][pGang]][gStashZ]))
