	if(!IsPlayerInTuneArea(playerid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "You are not in range of the tunning place.");
	}
	if((FactionInfo[pData[playerid][pFaction]][fType] != FACTION_MECHANIC))
	{
