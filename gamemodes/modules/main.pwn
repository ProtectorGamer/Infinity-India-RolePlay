CMD:rso(playerid, params[]) return callcmd::requestshoutout(playerid, params);
CMD:requestshoutout(playerid, params[])
{
	new string[128];
	if(isnull(params))
	{
	    return SCM(playerid, COLOR_SYNTAX, "Usage: /(R)equest(S)hout(O)ut [text]");
	}
	if(gettime() - pData[playerid][pLastShoutout] < 300)
	{
		return SM(playerid, COLOR_SYNTAX, "You cannot send another reuqest within 5 minutes. Please wait %i minutes, %i seconds and try again later.", 300 - (pData[playerid][pLastShoutout] - gettime()) / 60, 300 - (gettime() - pData[playerid][pLastShoutout]));
	}

	if(strlen(params) > MAX_SPLIT_LENGTH)
	{
		format(string, sizeof(string), "** Shout out request from "RED"%s: "WHITE"%.*s... **", GetRPName(playerid), MAX_SPLIT_LENGTH, params);
		SendShoutoutToInfluencer(COLOR_YELLOW, string);
		format(string, sizeof(string), "** %s: ...%s **", GetRPName(playerid), params[MAX_SPLIT_LENGTH]);
		SendShoutoutToInfluencer(COLOR_WHITE, string);
	}
	else
	{
	    format(string, sizeof(string), "** Shout out request from "RED"%s: "WHITE"%s **", GetRPName(playerid), params);
		SendShoutoutToInfluencer(COLOR_YELLOW, string);
	}
	return 1;
}

CMD:togMM(playerid, params[])
{
	new string[64];
	if(pData[playerid][pAdmin] < 5)
	{
		return SendClientMessage(playerid, COLOR_SYNTAX, "You are not authorized to use this command.");
	}
	if(!togMM)
	{
		format(string, sizeof(string), "AdmCmd: Administrator %s has enabled Multimedia Team Chat.", GetRPName(playerid));
		SendShoutoutToInfluencer(COLOR_LIGHTRED, string);
		togMM = 0;
	}
	else
	{
		format(string, sizeof(string), "AdmCmd: Administrator %s has disabled Multimedia Team Chat.", GetRPName(playerid));
		SendShoutoutToInfluencer(COLOR_LIGHTRED, string);
		togMM = 1;
	}
	return 1;
}

