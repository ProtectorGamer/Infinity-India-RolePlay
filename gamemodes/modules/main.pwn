public DCC_OnMessageCreate(DCC_Message:message){
 
    new DCC_Channel:channel;
 
    DCC_GetMessageChannel(message, channel);
 
    // This if condition checks that if the channel is not the command channel then it will do nothing and return 1 //
    if(channel != commandChannel)
        return 1;
 
    // But before that we need to save who actually sent the message //
    new DCC_User:author;
    DCC_GetMessageAuthor(message, author);
 
    // We need to check if the author is a bot or not //
 
    new bool:isBot;
    DCC_IsUserBot(author, isBot);
 
    // If the author is a bot then nothing will happen //
    if(isBot){      
        return 1;
    }
        
 
    // Now we will fetch if the author has the required role or not! //
    new bool:hasRole;
    DCC_HasGuildMemberRole(guildName, author, adminRole, hasRole);
 
    // Now to actually make a command after all the checks are done //
    new str[256];
    new command[32], params[128];
 
    DCC_GetMessageContent(message, str); // Storing the entire message of the author in the variable str //
 
    // Now seperating the command and the params //
 
    sscanf(str, "s[32]s[128]", command, params);
 
    // Now we will check the command //
 
 
    /*
    
        We will make a command which sends a chat message from discord to the player 
 
    */
	new DCC_Guild:guild = DCC_FindGuildById("905667779103977522");
	//new DCC_User:author;
	new authorName[32 + 1];
	DCC_GetMessageAuthor(message, author);
	//DCC_GetGuildMemberNickname(guild, author, authorName);
	DCC_GetGuildMemberNickname(guild, author, authorName, sizeof(authorName));
	if(isnull(authorName))
    	DCC_GetUserName(author, authorName, sizeof(authorName));

	
    if(!strcmp(command, "!g", true)){
        new _message[256], string[128];
		sscanf(params, "s[256]", _message); // Seperating the playerID and the message //
 
		foreach(new i : Player)
		{
			if(strlen(_message) > MAX_SPLIT_LENGTH)
			{
				format(string, sizeof(string), "(( Discord Admin %s: %.*s...))", authorName, MAX_SPLIT_LENGTH, _message);
				format(string, sizeof(string), "(( %s: ...%s ))", authorName, _message[MAX_SPLIT_LENGTH]);
				DCC_SendChannelMessage(g_chatlogs, string);
				SCMf(i, 0x4F9C4FFF, string);
			}
			else
			{
				format(string, sizeof(string), "(( Discord Admin %s: %s ))", DCC_GetUserName(author, authorName, sizeof(authorName)), _message);
				DCC_SendChannelMessage(g_chatlogs, string);
				SCMf(i, 0x4F9C4FFF, string);
			}
		}
    }
	if(!strcmp(command, "!kick", true)){
		new playerID, _message[128];
 
        sscanf(params, "us[128]", playerID, _message); // Seperating the playerID and the message //
 
        if(!IsPlayerConnected(playerID))
            return DCC_SendChannelMessage(channel, "The player is not online!");

		Kick(playerID);
		SMA(COLOR_LIGHTRED, "AdmCmd: %s has been kicked by a Discord Mod/Admin. Reason: %s", GetRPName(playerID), _message);
    }
	if(!strcmp(command, "!ban", true)){
		new playerID, _message[128];
 
        sscanf(params, "us[128]", playerID, _message); // Seperating the playerID and the message //
 
        if(!IsPlayerConnected(playerID))
            return DCC_SendChannelMessage(channel, "The player is not online!");

		BanPlayer(playerID, GetPlayerNameEx(playerID), _message);
		SMA(COLOR_LIGHTRED, "AdmCmd: %s has been banned by a Discord Admin(%s). Reason: %s", GetRPName(playerID), authorName, _message);
    }
	if(!strcmp(command, "!permaban", true)){
		new playerID, _message[128];
 
        sscanf(params, "us[128]", playerID, _message); // Seperating the playerID and the message //
 
        if(!IsPlayerConnected(playerID))
            return DCC_SendChannelMessage(channel, "The player is not online!");

		BanPlayer(playerID, GetPlayerNameEx(playerID), _message, true);
		SMA(COLOR_LIGHTRED, "AdmCmd: %s has been permanent banned by a Discord Admin(%s). Reason: %s", GetRPName(playerID), authorName, _message);
    }
	if(!strcmp(command, "!ip", true)){
		DCC_SendChannelMessage(channel, "`159.223.90.51:2020`");
    }
 
    // DONE !!! //
 
    return 1;
}

