CMD:makeMM(playerid, params[])
{
	new targetid, level, string[128];

    if(pData[playerid][pAdmin] < 6 && !pData[playerid][pDeveloper])
	{
	    return SCM(playerid, COLOR_GREY, "You are not authorized to use this command.");
	}
	if(sscanf(params, "ui", targetid, level))
	{
	    return SCM(playerid, COLOR_SYNTAX, "Usage: /make(M)ulti(M)edia [playerid] [level]");
	}
	if(!IsPlayerConnected(targetid))
	{
	    return SCM(playerid, COLOR_SYNTAX, "The player specified is disconnected.");
	}
	if(!pData[targetid][pLogged])
	{
	    return SCM(playerid, COLOR_SYNTAX, "That player hasn't logged in yet.");
	}
	if(!(0 <= level <= 3))
	{
	    return SCM(playerid, COLOR_SYNTAX, "Invalid level. Valid levels range from 0 to 3.");
	}
	if(level == 0)
	{
		
		SetPlayerSpecialTag(targetid, TAG_NORMAL);
	}


	format(string, sizeof(string), "AdmCmd: %s has made %s a part of MultiMedia Team. Level: %i .", GetRPName(playerid), GetRPName(targetid), level);
	SendShoutoutToInfluencer(COLOR_LIGHTRED, string);
	pData[targetid][pStaff] = level;

	mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET multimedia = %i WHERE uid = %i", level, pData[targetid][pID]);
	mysql_tquery(connectionID, queryBuffer);

	SM(playerid, COLOR_AQUA, "You have made %s a part of Multimedia Team level "SVRCLR"%s{CCFFFF} (%i).", GetRPName(targetid), GetMMRank(targetid), level);
	SM(targetid, COLOR_AQUA, "%s has made you a  part of Multimedia Team level "SVRCLR"%s{CCFFFF} (%i).", GetRPName(playerid), GetMMRank(targetid), level);

	Log_Write("log_admin", "%s (uid: %i) set %s's (uid: %i) multimedia team level to %i", GetPlayerNameEx(playerid), pData[playerid][pID], GetPlayerNameEx(targetid), pData[targetid][pID], level);
	return 1;
}

