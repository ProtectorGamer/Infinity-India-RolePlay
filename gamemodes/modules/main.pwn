	    mysql_format(connectionID, queryBuffer, sizeof(queryBuffer), "UPDATE users SET dynamicadmin = %i WHERE uid = %i", pData[targetid][pDynamicAdmin], pData[targetid][pID]);
	    mysql_tquery(connectionID, queryBuffer);

	    if(status)
