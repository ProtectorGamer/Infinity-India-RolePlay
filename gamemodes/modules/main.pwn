			}
			else if(IsPlayerInRangeOfPoint(issuerid, SafezoneInfo[Sz][szSize], SafezoneInfo[Sz][szPosX], SafezoneInfo[Sz][szPosY], SafezoneInfo[Sz][szPosZ]))
  			{
   				Dyuze(issuerid, "", "You are in the ~r~parameter~w~ of the ~g~Safezone", 5000);
   				return 0;
			}
 		}
	}
	return 1;
}
