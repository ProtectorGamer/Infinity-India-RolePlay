stock LoadLabels()
{
	if(!fexist("Labels.cfg")) return 1;

	new string[128],
	szFileStr[128],
	File: iFileHandle = fopen("Labels.cfg", io_read),
	iIndex;

	while(iIndex < sizeof(LabelInfo) && fread(iFileHandle, szFileStr))
	{
		sscanf(szFileStr, "p<|>fffsi",
		LabelInfo[iIndex][labelPosX],
		LabelInfo[iIndex][labelPosY],
  		LabelInfo[iIndex][labelPosZ],
  		LabelInfo[iIndex][labelSize],
  		LabelInfo[iIndex][labelPickupID]
	);
		format(string, sizeof(string), "%s\nID: %d", LabelInfo[iIndex][labelSize], iIndex);
		if(LabelInfo[iIndex][labelPosX] != 0.0)
		{
			LabelInfo[iIndex][labelPickupID] = CreateDynamicPickup(19134, 23, LabelInfo[iIndex][labelPosX], LabelInfo[iIndex][labelPosY], LabelInfo[iIndex][labelPosZ]);
			LabelInfo[iIndex][labelTextID] = CreateDynamic3DTextLabel(string, -1, LabelInfo[iIndex][labelPosX], LabelInfo[iIndex][labelPosY], LabelInfo[iIndex][labelPosZ]+0.5,30.0, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, -1, -1,  -1, 30.0);
		}
		++iIndex;
	}
	return fclose(iFileHandle);
}

