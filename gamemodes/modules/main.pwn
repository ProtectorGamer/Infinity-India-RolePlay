		SetTimerEx("OnTaserShoot", 1100, false, "i", playerid);
	    PlayerPlaySound(playerid, 6003, 0.0, 0.0, 0.0);
		SetTimerEx("RechargeTazer", 2000, false, "i", playerid);
		SCM(playerid, COLOR_SYNTAX, "Recharging tazer, please wait...");
